<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	ui:generateFormat='com.google.gwt.i18n.rebind.format.PropertiesFormat'
	ui:generateKeys="com.google.gwt.i18n.rebind.keygen.MD5KeyGenerator"
	ui:generateLocales="default" xmlns:g="urn:import:com.google.gwt.user.client.ui">

	<ui:image field="mainBackground" src='../mainBackground.png' />

	<ui:style>
		@sprite .main {
			gwt-image: "mainBackground";
			background-position: center center;
			background-repeat: no-repeat;
			width: 100%;
		}
	</ui:style>

	<g:HTMLPanel styleName="{style.main}">
		<div class="main">
			<div class="error col">
				<ui:msg description="Failed to load">Failed to load
					application, please try
					later.</ui:msg>
			</div>
		</div>
	</g:HTMLPanel>
</ui:UiBinder>