<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:a="urn:import:cz.artique.client.artique">

	<g:DockPanel stylePrimaryName="row">
		<g:Dock direction="NORTH">
			<g:Label ui:field="title" stylePrimaryName="title" />
		</g:Dock>
		<g:Dock direction="CENTER">
			<g:Label ui:field="content" stylePrimaryName="content" />
		</g:Dock>
	</g:DockPanel>
</ui:UiBinder>