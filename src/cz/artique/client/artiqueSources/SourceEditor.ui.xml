<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:a="urn:import:cz.artique.client.artiqueSources"
	xmlns:db="urn:import:com.google.gwt.user.datepicker.client">

	<ui:with field='res' type='cz.artique.client.i18n.ArtiqueConstants' />

	<g:Grid ui:field="grid">
		<g:row>
			<g:cell>
				<ui:text from='{res.sourceType}' />
			</g:cell>
			<g:customCell>
				<a:SourceTypePicker ui:field="sourceType" />
			</g:customCell>
		</g:row>
		<g:row>
			<g:cell>
				<ui:text from='{res.url}' />
			</g:cell>
			<g:customCell>
				<g:TextBox ui:field="url" />
			</g:customCell>
		</g:row>
		<g:row>
			<g:cell>
				<ui:text from='{res.region}' />
			</g:cell>
			<g:customCell>
				<a:SourceRegionPicker ui:field="region" />
			</g:customCell>
		</g:row>
	</g:Grid>
</ui:UiBinder>